programa {
	funcao inicio() {
	    /*
	    Leia dois valores inteiros. A seguir, calcule o produto entre estes dois valores e
        atribua esta operação à variável prod. A seguir mostre a variável prod com
        mensagem correspondente.
	    */
	    inteiro a,b, prod
	    leia (a)
	    leia (b)
	    prod=a*b
	    escreva ("PROD = ", prod)
	}
}
