programa {
	funcao inicio() {
	    /*
	    Leia 4 valores reais que correspondem a 4 notas de um estudante. A seguir,
        calcule a média do estudante.
	    */
	    real a,b,c,d, media
	    leia (a)
	    leia (b)
	    leia (c)
	    leia (d)
	    media=(a+b+c+d)/4
	    escreva ("MEDIA = ", media)
	}
}
