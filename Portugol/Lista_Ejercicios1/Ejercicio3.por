programa {
	funcao inicio() {
	    /*
	    Leia dois valores inteiros, no caso para variáveis a e b. A seguir, calcule a soma
        entre elas e atribua à variável soma. A seguir, exiba o valor desta variável.
	    */
	    inteiro a,b, soma
	    leia (a)
	    leia (b)
	    soma=a+b
	    escreva ("SOMA = ", soma)
	}
}
