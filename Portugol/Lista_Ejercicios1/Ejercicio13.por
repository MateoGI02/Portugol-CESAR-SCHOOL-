programa {
	funcao inicio() {
		/*
		Você quer saber o valor em real de um produto em uma loja virtual que está
        precificado em dólar. Faça um programa que leia a cotação do dólar e faça a
        conversão em real
        */
		real dolar, valor, final
		leia (dolar)
		leia (valor)
        final=dolar*valor
		escreva ("Valor em real: R$",final)
	}
}
